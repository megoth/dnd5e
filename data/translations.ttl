prefix :         <#>
prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#>
prefix skos:     <http://www.w3.org/2004/02/skos/core#>

:appName
  a rdfs:Literal;
  skos:definition "D&D5E Solid App"@en-US.

:appDescription
  a rdfs:Literal;
  skos:definition "A Solid App for D&D5E"@en-US.

:loggedIn
  a rdfs:Literal ;
  skos:definition "Logged in as {$name}"@en-US.

:loggedOut
  a rdfs:Literal ;
  skos:definition "Not logged in"@en-US.

:logIn
  a rdfs:Literal ;
  skos:definition "Log in"@en-US.

:logOut
  a rdfs:Literal ;
  skos:definition "Log out"@en-US.
